package co.edu.unbosque.model;

public class BetDTO {

	private int day;
	private int month;
	private int year;

	private double betPlaced;

	public BetDTO() {
		// TODO Auto-generated constructor stub
	}

	public BetDTO(int day, int month, int year, double betPlacced) {
		super();
		this.day = day;
		this.month = month;
		this.year = year;
		this.betPlaced = betPlacced;
	}

	public int getDay() {
		return day;
	}

	public void setDay(int day) {
		this.day = day;
	}

	public int getMonth() {
		return month;
	}

	public void setMonth(int month) {
		this.month = month;
	}

	public int getYear() {
		return year;
	}

	public void setYear(int year) {
		this.year = year;
	}

	public double getBetPlacced() {
		return betPlaced;
	}

	public void setBetPlacced(double betPlaced) {
		this.betPlaced = betPlaced;
	}

	@Override
	public String toString() {
		return "BetDTO [day=" + day + ", month=" + month + ", year=" + year + ", betPlaced=" + betPlaced + "]";
	}

}
